{
  "name": "health-interoperability-hub",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ssr": "ng build && ng run health-interoperability-hub:server",
    "serve:ssr": "node dist/health-interoperability-hub/server/main.js",
    "prerender": "ng run health-interoperability-hub:prerender",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:changed": "jest --passWithNoTests --onlyChanged || echo 'No tests to run'",
    "format": "prettier --write \"src/**/*.{ts,html,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,css,scss}\"",
    "lint": "eslint . --ext .ts,.html --max-warnings=0",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "*.{ts,html,css,scss}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/platform-server": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@angular/ssr": "^18.2.10",
    "@ngrx/effects": "^18.1.1",
    "@ngrx/signals": "^18.1.1",
    "@ngrx/store": "^18.1.1",
    "@ngrx/store-devtools": "^18.1.1",
    "@tailwindcss/postcss": "^4.1.8",
    "express": "^4.18.2",
    "lucide-angular": "^0.511.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.10",
    "@angular/cli": "^18.2.19",
    "@angular/compiler-cli": "^18.2.0",
    "@compodoc/compodoc": "^1.1.26",
    "@storybook/addon-docs": "^9.0.4",
    "@storybook/addon-onboarding": "^9.0.4",
    "@storybook/angular": "^9.0.4",
    "@storybook/builder-vite": "^9.0.4",
    "@testing-library/angular": "^17.3.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.18.0",
    "angular-eslint": "19.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-storybook": "^9.0.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.5",
    "lint-staged": "^16.1.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "storybook": "^9.0.4",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.32.1"
  }
}
